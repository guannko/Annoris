    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.3",
    "autocannon": "^7.14.0"
